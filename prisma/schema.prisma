generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model users {
  id    String  @id
  email String  @unique(map: "unique_email")
  name  String?
  emoji String? @db.Char(4)
}
